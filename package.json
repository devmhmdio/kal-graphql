{
  "name": "kalendar-ai-clone-graphql",
  "version": "1.0.0",
  "description": "Kalendar Clone GraphQL",
  "main": "dist/server.js",
  "scripts": {
    "start": "npm run build && node dist/server.js",
    "build": "tsc",
    "start:dev": "npm run build:dev",
